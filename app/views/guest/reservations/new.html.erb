<%= form_with model: @reservation, url: guest_space_confirm_path, local: true do |f| %>
  <%= f.label "利用開始日時" %>
  <%= f.datetime_select :start_time, start_hour: @space.open_time.hour, end_hour: @space.close_time.hour, minute_step: 30 %>
  <br>
  <%= f.label "利用終了日時" %>
  <%= f.datetime_select :end_time, discard_year: true, discard_month: true, start_hour: @space.open_time.hour, end_hour: @space.close_time.hour, minute_step: 30 %>
<% end %>